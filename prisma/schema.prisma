generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Items {
  id          String   @id @default(cuid())
  name        String?
  description String
  tags        String?
  background  String?
  deleted     Boolean  @default(false)
  archived    Boolean  @default(false)
  createdAt   DateTime @default(now())
  public      Boolean  @default(false)
  owner       String
}

model Labels {
  id        String   @id @default(cuid())
  name      String
  color     String
  createdAt DateTime @default(now())
  owner     String
}

model ItemLabelsAdded {
  itemId  String
  labelId String
  id      String @id @default(cuid())
}

model LikedItems {
  id   String @id @default(cuid())
  item String
  user String
}
